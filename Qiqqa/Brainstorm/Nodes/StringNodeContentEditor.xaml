<UserControl x:Class="Qiqqa.Brainstorm.Nodes.StringNodeContentEditor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    >
  <StackPanel>
  <TextBlock Text="Node text:"/>
  <TextBox TextWrapping="Wrap" Text="{Binding Text}" AcceptsReturn="True" AcceptsTab="True"/>
  </StackPanel>
</UserControl>
