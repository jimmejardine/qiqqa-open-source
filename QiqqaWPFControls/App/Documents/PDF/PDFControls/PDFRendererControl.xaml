<UserControl x:Class="Qiqqa.Documents.PDF.PDFControls.PDFRendererControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local_gui="clr-namespace:Utilities.GUI"
>

  <Grid Width="Auto" Height="Auto" Name="PageRenderArea">
    <local_gui:AugmentedScrollViewer Name="ScrollPages" CanContentScroll="True" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto">
      <local_gui:AugmentedWrapPanel Name="ObjPagesPanel" />
    </local_gui:AugmentedScrollViewer>
  </Grid>
</UserControl>
